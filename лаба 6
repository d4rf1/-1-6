m = 10  # задаем размер массива
A = [i for i in range(1, m+1)]  # создаем исходный массив A

print("Исходный массив A:")
print(A)

for i in range(1, m, 2):
    A[i] = 0  # заменяем каждый второй элемент на 0

S = sum(A)  # вычисляем сумму оставшихся элементов

print("Преобразованный массив A:")
print(A)
print(f"Сумма элементов массива A: {S}")
