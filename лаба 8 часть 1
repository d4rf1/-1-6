S = 26
G = 6
K = 80301

# Коэффициенты перед переменными в уравнениях
a = 1
b = 5
c = 1
d = 4
e = -1
f = 1
g = S
h = G
i = 2*(S+G+K)
j = 2*((S-G)*(S+G+K)+G)+K*S

# Решение системы уравнений
x3 = (j - i * (b*h - e*g) - c * (f*h - i*e)) / (a * (f*h - i*e) - b * (c*h - i*d))
x2 = (j - i * x3 - c * x3) / (f*h - i*e)
x1 = (j - i * x3 - f * x2) / i

print("Решение системы уравнений:")
print("x1 =", x1)
print("x2 =", x2)
print("x3 =", x3)
