N = 10
X=[0.1, 0.3, 0.4, 0.7, 1]
for i in range(0,5):
    x = float()
    X.append(x)
print(' -------------------------')
print(' | x         |         s |')
print(' -------------------------')

for x in X:
    s = 0
    u = -x
    for k in range(1,N+1):
        s += u/k
    print(" | %8.4f | %8.4f |" % (x,s))

print(' -------------------------')
